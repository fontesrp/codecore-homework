<%- include("../partials/header") %>

<div class="container">
    <form action="/cohorts" method="post">
        <div class="form-group">
            <label for="logo">Logo URL</label>
            <input type="text" class="form-control" name="logo" id="logo" placeholder="e.g. https://i.imgur.com/kA04vQP.png">
        </div>
        <div class="form-group">
            <label for="name">Name</label>
            <input type="text" class="form-control" name="name" id="name" placeholder="Flying Funks">
        </div>
        <div class="form-group">
            <label for="members">Members</label>
            <textarea class="form-control" name="members" id="members" placeholder="Jon Snow, Daenerys, Melisandre, Arya Stark"></textarea>
        </div>
        <button type="submit" class="btn btn-outline-primary flex-row ml-auto d-flex ">Submit</button>
    </form>
</div>

<%- include("../partials/footer") %>
